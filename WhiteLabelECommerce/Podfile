pod 'SwiftLint'

target 'WhiteLabelECommerce (iOS)' do
  platform :ios, '15.0'
  use_frameworks!

  target 'WhiteLabelECommerceTests(iOS)' do
    inherit! :search_paths
  end
end

target 'WhiteLabelECommerce (macOS)' do
  platform :osx, '11.0'
  use_frameworks!
  target 'WhiteLabelECommerceTests(mac)' do
    inherit! :search_paths
  end
end
